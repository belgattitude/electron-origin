{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    [
      "env",
      {
        "targets": { "node": 7 },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-es2015-spread",
    "react-hot-loader/babel"
  ]
}