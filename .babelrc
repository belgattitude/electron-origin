{
  "presets": [
    ["env", {
        "targets": {
          "node": 7,
          "browsers": ["last 2 versions", "safari >= 7"]
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-es2015-spread",
    "react-hot-loader/babel"
  ]
}